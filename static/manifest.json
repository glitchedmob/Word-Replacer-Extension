{
  "manifest_version": 2,
  "name": "Word Replacer",
  "description": "Replace words on the web",
  "version": "1.0",
  "browser_action": {
    "default_popup": "index.html#/popup"
  },
  "permissions": [
    "*://*/",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "options_page": "index.html#/options"
}
